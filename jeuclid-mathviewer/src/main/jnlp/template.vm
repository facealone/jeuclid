<?xml version="1.0" encoding="utf-8"?>
<jnlp
    spec="1.0+"
    codebase="http://jeuclid.sf.net/jeuclid-mathviewer"
    href="$outputFile">
  <information>
    <title>JEuclid MathViewer</title>
    <vendor>JEuclid developers<vendor/>
    <homepage>$project.url<homepage/>
    <description kind="short">$informationDescription</description>
    <icon href="jeuclid.gif" />
    <offline-allowed/>
    <association mime-type="application/mathml+xml" extensions="mml" />
    <shortcut online="false">
      <desktop/>
      <menu submenu="JEuclid"/>
    </shortcut>
  </information>
  <security>
     <all-permissions/>
  </security>
  <resources>
    <j2se version="1.6+"/>
     $dependencies
  </resources>
  <application-desc main-class="$mainClass" />
</jnlp>

